  "overrides": {
    "next": "16.2.0-canary.51",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "private": true,
  "scripts": {
    "build": "next build",
    "check": "biome check --write .",
    "db:admin": "dotenv -e .env -- bun prisma/seed/admin.ts",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:status": "dotenv -e .env -- prisma migrate status",
    "db:push": "prisma db push",
    "db:reset": "dotenv -e .env -- prisma migrate reset --force",
    "db:seed": "dotenv -e .env -- prisma db seed",
    "db:studio": "dotenv -e .env -- prisma studio",
    "dev": "next dev",
    "format": "biome check --write --unsafe .",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "minio:mc": "mc alias set localminio http://localhost:9000 drhazem HealthF26",
    "pre-commit": "bun format && bun lint:fix",
    "shadcn:add": "bunx shadcn@latest add",
    "shadcn:diff": "bunx shadcn@latest diff",
    "shadcn:update": "bunx shadcn@latest add --overwrite",
    "shadcn:update-all": "./scripts/update-shadcn.sh",
    "start": "next start",
    "start:infra": "npm run start:redis && npm run start:minio",
    "start:minio": "bash ./scripts/run-minio.sh",
    "start:redis": "bash ./scripts/run-redis.sh",
    "stop:infra": "npm run stop:redis && npm run stop:minio",
    "stop:minio": "podman stop minio && podman rm minio",
    "stop:redis": "podman stop redis && podman rm redis",
    "typecheck": "NODE_OPTIONS='--max-old-space-size=8192' tsc --noEmit"
  },
  "trustedDependencies": [
    "sharp",
    "unrs-resolver"
  ],
  "version": "0.1.0"
